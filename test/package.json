{
  "name": "bedrock-test-test",
  "version": "0.0.1-0",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "node --preserve-symlinks test test",
    "coverage": "cross-env NODE_ENV=test c8 --check-coverage --reporter=lcov --reporter=text-summary npm test",
    "coverage-ci": "cross-env NODE_ENV=test c8 --check-coverage --reporter=lcovonly npm test",
    "coverage-report": "c8 report"
  },
  "dependencies": {
    "@bedrock/core": "file:..",
    "@bedrock/test": "^7.0.0"
  },
  "devDependencies": {
    "c8": "^7.11.0",
    "cross-env": "^7.0.2"
  },
  "nyc": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/bedrock/**"
    ],
    "exclude": [
      "node_modules/bedrock/node_modules/**"
    ]
  }
}
